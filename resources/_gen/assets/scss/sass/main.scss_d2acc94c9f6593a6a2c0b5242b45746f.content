x}#content pre code{all:unset}#content .highlight{margin:1rem auto}#content .highlight>pre{padding:8px}.logo img{align:right;max-width:150px;max-height:150px}.navbar{align-items:left}.logo-container{position:relative;display:inline-flex;align-items:center;margin-right:10px}.logo img{max-width:50;max-height:50px}.logo-outside{left:-5px;top:50%;transform:translateY(-50%)}.people.container{padding:40px;text-align:center;max-width:700px;margin:0 auto}.people-list{display:flex;flex-wrap:wrap;justify-content:center;gap:30px}.person{width:300px;text-align:center;border-radius:10px;padding:20px;transition:transform .3s ease,box-shadow .3s ease}.person-icon{width:150px;height:150px;border-radius:50%;object-fit:cover;margin-bottom:15px}.person-name{font-size:1.2em;font-weight:600;color:#222;margin-bottom:10px}.person-position{font-size:1em;font-weight:400;color:#222;margin-bottom:10px}@media(max-width:768px){.people-list{flex-direction:column;align-items:center}.person{width:100%;max-width:300px}}.article-list{padding:0;max-width:560px;margin:0 auto}.article-list .article{padding:0;margin:0 0 5px;border:1px solid #e1e1e1;transition:transform .3s ease,box-shadow .3s ease}.article-list ul.article-links{padding:0;margin:5px}.article-list li{padding:0;margin:5px;list-style:none}.article-list .article-title{font-size:1.2em;font-weight:600;color:#222;margin-bottom:2px}.article-list .article-author{font-size:1em;font-weight:400;color:#222;margin-bottom:2px}.article-list .article-doi{font-size:1em;font-weight:300;color:#f90}